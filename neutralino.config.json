{
  "applicationId": "create-react-neutralino-app",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "neutralinojs app with reactjs",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/public/logo192.png",
      "borderless": false
    }
  },
  "cli": {
    "binaryName": "neutralinojs",
    "resourcesPath": "/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/build/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0",
    "frontendLibrary": {
      "patchFile": "/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}